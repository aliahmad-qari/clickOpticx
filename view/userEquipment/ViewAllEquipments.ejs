    .form-label {
      position: absolute;
      top: 12px;
      left: 0;
      font-weight: 500;
      color: black;
      pointer-events: none;
      transition: all 0.3s ease;
    }

    .form-control:focus~.form-label,
    .form-control:not(:placeholder-shown)~.form-label,
    .form-select:focus~.form-label,
    .form-select:not([value=""])~.form-label {
      top: -20px;
      left: 0;
      font-size: 0.85rem;
      color: var(--primary);
    }

    .form-control:focus~.form-label::after,
    .form-control:not(:placeholder-shown)~.form-label::after,
    .form-select:focus~.form-label::after,
    .form-select:not([value=""])~.form-label::after {
      content: '*';
      color: var(--danger);
      margin-left: 5px;
    }

    .form-select option {
      background: var(--light);
      color: var(--dark);
    }

    .btn-primary {
      background-color: #1a8754;
      border: none;
      padding: 15px;
      border-radius: 50px;
      font-weight: 600;
      color: white;
      /* width: 100%; */
      cursor: pointer;
      transition: all 0.3s ease;
      position: relative;
      overflow: hidden;
    }

    .btn-primary:hover {
      transform: translateY(-3px);
      box-shadow: 0 8px 20px rgba(108, 92, 231, 0.3);
    }

    .btn-primary::before {
      content: '';
      position: absolute;
      top: 0;
      left: -100%;
      width: 100%;
      height: 100%;
      background: linear-gradient(90deg, transparent, rgba(255, 255, 255, 0.2), transparent);
      animation: shine 2s infinite;
    }

    .form-icon {
      position: absolute;
      right: 10px;
      top: 50%;
      transform: translateY(-50%);
      color: var(--gray);
      transition: color 0.3s ease;
    }

    .form-control:focus~.form-icon,
    .form-select:focus~.form-icon {
      color: var(--primary);
    }

    .equipment-image {
      max-height: 100px;
      margin-top: 10px;
      border-radius: 8px;
      box-shadow: 0 4px 10px rgba(0, 0, 0, 0.1);
    }

    /* Modal Styling */


    /* Responsive Adjustments */
    @media (max-width: 768px) {
      .toolbar {
        flex-direction: column;
        align-items: stretch;
      }

      .search-box {
        max-width: 100%;
      }

      .filter-options {
        flex-direction: column;
        align-items: stretch;
      }

      .btn-add {
        justify-content: center;
      }

      .table-footer {
        flex-direction: column;
        gap: 15px;
        align-items: center;
      }
    }
  </style>
</head>

<body>
  <div class="container-scroller">
    <!-- partial:partials/_navbar -->
    <%- include ('../partials/_navbar.ejs') %>
      <!-- partial:partials/_navbar -->

      <!-- partial -->
      <div class="container-fluid page-body-wrapper">
        <!-- NavBar Start -->
        
          <%- include ('../partials/Sidebar.ejs') %>
            <!-- NavBar End -->

            <!-- Main Content -->
            <div class="main-panel">
              <div class="content-wrapper">
                <div class="card">
                  <div class="card-header">
                    <i class="fas fa-tools mr-2"></i> Equipment Inventory
                    <div class="header-actions">
                      <button class="btn-refresh">
                        <i class="fas fa-sync-alt"></i>
                      </button>
                    </div>
                  </div>

                  <div class="card-body">
                    <!-- Calculate pagination display variables -->
                    <% const currentPage=parseInt(page) || 1; const startIndex=(currentPage - 1) * entriesPerPage + 1;
                      const endIndex=Math.min(startIndex + equipment.length - 1, totalEntries); const
                      buildQueryString=(pageNum)=>
                      {
                      const params = new URLSearchParams();
                      if (search) params.set('search', search);
                      if (category) params.set('category', category);
                      params.set('page', pageNum);
                      return `/ViewAllEquipments?${params.toString()}`;
                      };
                      %>

                      <% if (locals.flash && locals.flash.success) { %>
                        <div class="alert alert-success">
                          <i class="fas fa-check-circle mr-2"></i>
                          <%= locals.flash.success %>
                        </div>
                        <% } %>

                          <% if (error) { %>
                            <div class="alert alert-danger">
                              <i class="fas fa-exclamation-circle mr-2"></i>
                              <%= error %>
                            </div>
                            <% } %>

                              <div class="toolbar">
                                <div class="search-box" style="position: relative;">
                                  <i class="fas fa-search"></i>
                                  <input type="text" id="equipment-search-input" placeholder="Search equipment by name, type, category..."
                                    class="search-input" autocomplete="off">
                                </div>

                                <div class="filter-options">
                                  <form action="/ViewAllEquipments" method="GET" id="categoryFilterForm">
                                    <div class="d-flex flex-wrap gap-2">
                                      <!-- Category Filter -->
                                      <select name="category" class="form-select" onchange="this.form.submit()">
                                        <option value="">All Categories</option>
                                        <option value="Fibre Optics" <%=category==='Fibre Optics' ? 'selected' : '' %>
                                          >Fibre
                                          Optics</option>
                                        <option value="Wireless Devices" <%=category==='Wireless Devices' ? 'selected'
                                          : '' %>>Wireless Devices</option>
                                      </select>

                                      <!-- Stock Status Filter -->
                                      <select name="stockStatus" class="form-select" onchange="this.form.submit()">
                                        <option value="">All Stock</option>
                                        <option value="In Stock" <%=stockStatus==='In Stock' ? 'selected' : '' %>>In
                                          Stock
                                        </option>
                                        <option value="Low Stock" <%=stockStatus==='Low Stock' ? 'selected' : '' %>>Low
                                          Stock</option>
                                        <option value="Out of Stock" <%=stockStatus==='Out of Stock' ? 'selected' : ''
                                          %>
                                          >Out of Stock</option>
                                      </select>
                                    </div>

                                    <!-- Preserve search and page -->
                                    <input type="hidden" name="search" value="<%= search %>">
                                    <input type="hidden" name="page" value="<%= currentPage %>">
                                  </form>

                                  <!-- Add Equipment Button -->
                                  <a href="/AddNewEquipment">
                                    <button class="btn-add">
                                      <i class="fas fa-plus mr-2"></i> Add Equipment
                                    </button>
                                  </a>
                                </div>
                              </div>
                              <!-- Desktop Table View -->
                              <div class="desktop-table">
                                <div class="table-responsive">
                                  <table class="equipment-table">
                                  <thead>
                                    <tr>
                                      <th class="name-col">Equipment Name</th>
                                      <th class="type-col">Type</th>
                                      <th class="category-col">Category</th>
                                      <th class="quantity-col">Quantity</th>
                                      <th class="status-col">Status</th>
                                      <th class="actions-col">Actions</th>
                                    </tr>
                                  </thead>
                                  <tbody>
                                    <% if (equipment.length> 0) { %>
                                      <% equipment.forEach(item=> { %>
                                        <% // Parse the numeric part of the quantity for status and bar calculations let
                                          quantityNumber=0; if (item.quantity) { const match=item.quantity.match(/\d+/);
                                          quantityNumber=match ? parseInt(match[0]) : 0; } %>
                                          <tr>
                                            <td class="name-col">
                                              <div class="equipment-info">
                                                <img class="equipment-image"
                                                  src="<%= item.image ? '/' + item.image : '/images/default-equipment.png' %>">
                                                <div>
                                                  <div class="equipment-name">
                                                    <%= item.name || 'Unnamed' %>
                                                  </div>
                                                  <div class="equipment-id">ID: <%= item.id || 'N/A' %>
                                                  </div>
                                                </div>
                                              </div>
                                            </td>
                                            <td class="type-col">
                                              <%= item.type || 'N/A' %>
                                            </td>
                                            <td class="category-col">
                                              <span class="category-badge">
                                                <%= item.category || 'N/A' %>
                                              </span>
                                            </td>
                                            <td class="quantity-col">
                                              <div class="quantity-display">
                                                <div class="quantity-value">
                                                  <%= item.quantity || '0' %>
                                                </div>
                                                <% if (quantityNumber> 0) { %>
                                                  <div class="quantity-bar"
                                                    style="width: <%= Math.min(100, (quantityNumber / 20) * 100) %>%">
                                                  </div>
                                                  <% } %>
                                              </div>
                                            </td>
                                            <td class="status-col">
                                              <% if (quantityNumber> 10) { %>
                                                <span class="status-badge in-stock">
                                                  <i class="fas fa-check-circle"></i> In Stock
                                                </span>
                                                <% } else if (quantityNumber> 0) { %>
                                                  <span class="status-badge low-stock">
                                                    <i class="fas fa-exclamation-triangle"></i> Low Stock
                                                  </span>
                                                  <% } else { %>
                                                    <span class="status-badge out-of-stock">
                                                      <i class="fas fa-times-circle"></i> Out of Stock
                                                    </span>
                                                    <% } %>
                                            </td>
                                            <td class="actions-col">
                                              <div class="action-buttons">
                                                <form action="/deleteEquipment/<%= item.id %>" method="POST"
                                                  onsubmit="return confirm('Are you sure you want to delete this equipment?');">
                                                  <button type="submit" class="btn-action view" title="Delete">
                                                    <i class="bi bi-trash" style="color:red" ;></i>
                                                  </button>
                                                </form>
                                                <!-- Update equipment -->
                                                <button class="btn-action edit" title="Edit" data-bs-toggle="modal"
                                                  data-bs-target="#editEquipmentModal" data-id="<%= item.id %>"
                                                  data-type="<%= item.type %>" data-name="<%= item.name %>"
                                                  data-quantity="<%= item.quantity %>"
                                                  data-category="<%= item.category %>" data-image="<%= item.image %>">
                                                  <i class="fas fa-edit" style="color:green;"></i>
                                                </button>
                                              </div>
                                            </td>
                                          </tr>
                                          <% }); %>
                                            <% } else { %>
                                              <tr class="empty-state">
                                                <td colspan="6">
                                                  <div class="empty-content">
                                                    <i class="fas fa-box-open"></i>
                                                    <h4>No Equipment Found</h4>
                                                    <p>Add new equipment to get started</p>
                                                    <a href="/AddNewEquipment">
                                                      <button class="btn-primary">
                                                        <i class="fas fa-plus mr-2"></i> Add Equipment
                                                      </button>
                                                    </a>
                                                  </div>
                                                </td>
                                              </tr>
                                              <% } %>
                                  </tbody>
                                  </table>
                                </div>
                              </div>

                              <!-- Mobile Cards View -->
                              <div class="mobile-cards">
                                <% if (equipment.length > 0) { %>
                                  <% equipment.forEach(item => { %>
                                    <% // Parse the numeric part of the quantity for status and bar calculations
                                    let quantityNumber = 0;
                                    if (item.quantity) {
                                      const match = item.quantity.match(/\d+/);
                                      quantityNumber = match ? parseInt(match[0]) : 0;
                                    }
                                    
                                    // Determine status class for mobile badge
                                    let statusClass = '';
                                    let statusText = '';
                                    if (quantityNumber > 10) {
                                      statusClass = 'in-stock';
                                      statusText = 'In Stock';
                                    } else if (quantityNumber > 0) {
                                      statusClass = 'low-stock';
                                      statusText = 'Low Stock';
                                    } else {
                                      statusClass = 'out-of-stock';
                                      statusText = 'Out of Stock';
                                    }
                                    %>
                                    
                                    <div class="equipment-card">
                                      <div class="equipment-card-header">
                                        <img class="equipment-card-image" 
                                             src="<%= item.image ? '/' + item.image : '/images/default-equipment.png' %>" 
                                             alt="<%= item.name || 'Unnamed' %>">
                                        <div class="equipment-card-name">
                                          <h6><%= item.name || 'Unnamed' %></h6>
                                          <p>ID: <%= item.id || 'N/A' %></p>
                                        </div>
                                      </div>
                                      
                                      <div class="equipment-card-content">
                                        <div class="equipment-card-field">
                                          <div class="equipment-card-label">Type</div>
                                          <div class="equipment-card-value">
                                            <%= item.type || 'N/A' %>
                                          </div>
                                        </div>
                                        
                                        <div class="equipment-card-field">
                                          <div class="equipment-card-label">Category</div>
                                          <div class="equipment-card-value">
                                            <span class="mobile-badge category"><%= item.category || 'N/A' %></span>
                                          </div>
                                        </div>
                                        
                                        <div class="equipment-card-field">
                                          <div class="equipment-card-label">Quantity</div>
                                          <div class="equipment-card-value">
                                            <%= item.quantity || '0' %>
                                          </div>
                                        </div>
                                        
                                        <div class="equipment-card-field">
                                          <div class="equipment-card-label">Status</div>
                                          <div class="equipment-card-value">
                                            <span class="mobile-badge <%= statusClass %>">
                                              <i class="fas fa-circle"></i> <%= statusText %>
                                            </span>
                                          </div>
                                        </div>
                                      </div>
                                      
                                      <div class="equipment-card-actions">
                                        <button class="mobile-btn view" onclick="viewEquipmentDetails('<%= item.id %>', '<%= item.name || 'Unnamed' %>', '<%= item.type || 'N/A' %>', '<%= item.category || 'N/A' %>', '<%= item.quantity || '0' %>', '<%= item.image ? '/' + item.image : '/images/default-equipment.png' %>')">
                                          <i class="fas fa-eye"></i> View
                                        </button>
                                        
                                        <button class="mobile-btn edit" data-bs-toggle="modal" data-bs-target="#editEquipmentModal" 
                                                data-id="<%= item.id %>" data-type="<%= item.type %>" data-name="<%= item.name %>" 
                                                data-quantity="<%= item.quantity %>" data-category="<%= item.category %>" data-image="<%= item.image %>">
                                          <i class="fas fa-edit"></i> Edit
                                        </button>
                                        
                                        <form action="/AdminEquipment/<%= item.id %>?_method=DELETE" method="POST" 
                                              onsubmit="return confirm('Are you sure you want to delete this equipment?');" 
                                              style="display: inline;">
                                          <input type="hidden" name="_method" value="DELETE">
                                          <button type="submit" class="mobile-btn delete">
                                            <i class="fas fa-trash"></i> Delete
                                          </button>
                                        </form>
                                      </div>
                                    </div>
                                  <% }) %>
                                <% } else { %>
                                  <div class="equipment-card">
                                    <div style="text-align: center; padding: 20px;">
                                      <i class="fas fa-box-open" style="font-size: 3rem; color: #ccc; margin-bottom: 10px;"></i>
                                      <h6>No equipment found</h6>
                                      <p>Add new equipment to get started</p>
                                      <a href="/AddNewEquipment">
                                        <button class="mobile-btn view">
                                          <i class="fas fa-plus"></i> Add Equipment
                                        </button>
                                      </a>
                                    </div>
                                  </div>
                                <% } %>
                              </div>

                              <div class="table-footer">
                                <div class="showing-entries">
                                  Showing <%= startIndex %> to <%= endIndex %> of <%= totalEntries %> entries
                                </div>
                                <div class="pagination">
                                  <a href="<%= currentPage > 1 ? buildQueryString(currentPage - 1) : '#' %>"
                                    class="page-btn <%= currentPage === 1 ? 'disabled' : '' %>">
                                    <i class="fas fa-chevron-left"></i>
                                  </a>
                                  <% for (let i=1; i <=totalPages; i++) { %>
                                    <a href="<%= buildQueryString(i) %>"
                                      class="page-btn <%= currentPage === i ? 'active' : '' %>">
                                      <%= i %>
                                    </a>
                                    <% } %>
                                      <a href="<%= currentPage < totalPages ? buildQueryString(currentPage + 1) : '#' %>"
                                        class="page-btn <%= currentPage === totalPages ? 'disabled' : '' %>">
                                        <i class="fas fa-chevron-right"></i>
                                      </a>
                                </div>
                              </div>
                  </div>
                </div>

                <!-- Edit Equipment Modal -->
                <div class="modal fade" id="editEquipmentModal" tabindex="-1" aria-labelledby="editEquipmentModalLabel"
                  aria-hidden="true">
                  <div class="modal-dialog">
                    <div class="modal-content form-card">
                      <div class="form-header">
                        <h5 class="modal-title" id="editEquipmentModalLabel">Edit Equipment</h5>
                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                      </div>
                      <div class="modal-body">
                        <form action="/updateEquipment" method="POST" enctype="multipart/form-data">
                          <input type="hidden" name="id" id="edit-id">

                          <div class="form-group">
                            <select name="type" id="edit-type" class="form-select" required>
                              <option value="" disabled selected hidden></option>
                              <option value="fibre">Fibre</option>
                              <option value="wireless">Wireless</option>
                            </select>
                            <label for="edit-type" class="form-label">Type</label>
                            <i class="fas fa-chevron-down form-icon"></i>
                          </div>

                          <div class="form-group">
                            <input type="text" name="name" id="edit-name" class="form-control" placeholder=" " required>
                            <label for="edit-name" class="form-label">Equipment Name</label>
                            <i class="fas fa-tag form-icon"></i>
                          </div>

                          <div class="form-group">
                            <input type="text" name="quantity" id="edit-quantity" class="form-control" placeholder=" "
                              required>
                            <label for="edit-quantity" class="form-label">Quantity</label>
                            <i class="fas fa-box form-icon"></i>
                          </div>

                          <div class="form-group">
                            <input type="text" name="category" id="edit-category" class="form-control" placeholder=" "
                              readonly>
                            <label for="edit-category" class="form-label">Category</label>
                            <i class="fas fa-list form-icon"></i>
                          </div>

                          <div class="form-group">
                            <label for="edit-image" class="form-label">Image (optional)</label>
                            <input type="file" name="image" id="edit-image" class="form-control" accept="image/*">
                            <div id="current-image-container" class="mt-2">
                              <img id="current-image" class="equipment-image" alt="Current Equipment Image">
                            </div>
                          </div>

                          <button type="submit" class="btn-primary">
                            <i class="fas fa-save mr-2"></i> Update Equipment
                          </button>
                        </form>
                      </div>
                    </div>
                  </div>
                </div>

                <!-- View Equipment Modal -->
                <div class="modal fade" id="viewEquipmentModal" tabindex="-1" aria-labelledby="viewEquipmentModalLabel" aria-hidden="true">
                  <div class="modal-dialog">
                    <div class="modal-content form-card">
                      <div class="form-header">
                        <h5 class="modal-title" id="viewEquipmentModalLabel">Equipment Details</h5>
                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                      </div>
                      <div class="modal-body">
                        <div class="row">
                          <div class="col-md-4 text-center mb-3">
                            <img id="view-equipment-image" src="" alt="Equipment Image" class="img-fluid rounded" style="max-height: 150px; object-fit: cover;">
                          </div>
                          <div class="col-md-8">
                            <table class="table table-borderless">
                              <tr>
                                <td><strong>Equipment ID:</strong></td>
                                <td id="view-equipment-id">-</td>
                              </tr>
                              <tr>
                                <td><strong>Name:</strong></td>
                                <td id="view-equipment-name">-</td>
                              </tr>
                              <tr>
                                <td><strong>Type:</strong></td>
                                <td id="view-equipment-type">-</td>
                              </tr>
                              <tr>
                                <td><strong>Category:</strong></td>
                                <td id="view-equipment-category">-</td>
                              </tr>
                              <tr>
                                <td><strong>Quantity:</strong></td>
                                <td id="view-equipment-quantity">-</td>
                              </tr>
                            </table>
                          </div>
                        </div>
                      </div>
                      <div class="modal-footer">
                        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
      </div>
  </div>

  <!-- category changes automatically -->
  <script>
    document.addEventListener('DOMContentLoaded', function () {
      const typeSelect = document.getElementById('edit-type');
      const categoryInput = document.getElementById('edit-category');

      // Function to update category based on selected type
      function updateCategory() {
        if (typeSelect.value === 'fibre') {
          categoryInput.value = 'Fibre Optics';
        } else if (typeSelect.value === 'wireless') {
          categoryInput.value = 'Wireless Devices';
        } else {
          categoryInput.value = '';
        }
      }

      // Trigger on change
      typeSelect.addEventListener('change', updateCategory);

      // Also run once on page load (or modal open)
      updateCategory();

      // Real-time search functionality for equipment
      const searchInput = document.getElementById('equipment-search-input');
      if (searchInput) {
        // Function to filter equipment table/cards based on search
        function filterEquipmentTable() {
          const searchTerm = searchInput.value.toLowerCase().trim();
          
          // Filter desktop table rows
          const tableRows = document.querySelectorAll('.equipment-table tbody tr');
          tableRows.forEach(row => {
            if (row.classList.contains('empty-state')) return;
            
            const equipmentName = row.querySelector('.equipment-name')?.textContent.toLowerCase() || '';
            const equipmentType = row.querySelector('.type-col')?.textContent.toLowerCase() || '';
            const equipmentCategory = row.querySelector('.category-badge')?.textContent.toLowerCase() || '';
            const equipmentQuantity = row.querySelector('.quantity-value')?.textContent.toLowerCase() || '';
            
            const matchesSearch = searchTerm === '' || 
              equipmentName.includes(searchTerm) ||
              equipmentType.includes(searchTerm) ||
              equipmentCategory.includes(searchTerm) ||
              equipmentQuantity.includes(searchTerm);
            
            row.style.display = matchesSearch ? '' : 'none';
          });

          // Filter mobile cards
          const mobileCards = document.querySelectorAll('.mobile-cards .equipment-card');
          mobileCards.forEach(card => {
            const equipmentName = card.querySelector('.equipment-card-name h6')?.textContent.toLowerCase() || '';
            const equipmentType = card.querySelector('.equipment-card-field .equipment-card-value')?.textContent.toLowerCase() || '';
            const equipmentCategory = card.querySelector('.mobile-badge.category')?.textContent.toLowerCase() || '';
            const equipmentQuantity = card.querySelectorAll('.equipment-card-value')[2]?.textContent.toLowerCase() || '';
            
            const matchesSearch = searchTerm === '' || 
              equipmentName.includes(searchTerm) ||
              equipmentType.includes(searchTerm) ||
              equipmentCategory.includes(searchTerm) ||
              equipmentQuantity.includes(searchTerm);
            
            card.style.display = matchesSearch ? '' : 'none';
          });

          // Show/hide empty state based on visible results
          updateEmptyState();
        }

        function updateEmptyState() {
          const visibleDesktopRows = document.querySelectorAll('.equipment-table tbody tr:not(.empty-state)');
          const visibleMobileCards = document.querySelectorAll('.mobile-cards .equipment-card');
          
          let hasVisibleItems = false;
          
          // Check desktop table
          visibleDesktopRows.forEach(row => {
            if (row.style.display !== 'none') {
              hasVisibleItems = true;
            }
          });
          
          // Check mobile cards
          if (!hasVisibleItems) {
            visibleMobileCards.forEach(card => {
              if (card.style.display !== 'none') {
                hasVisibleItems = true;
              }
            });
          }

          // Handle empty state for desktop table
          const emptyStateRow = document.querySelector('.equipment-table .empty-state');
          if (emptyStateRow) {
            emptyStateRow.style.display = hasVisibleItems ? 'none' : '';
          }
        }

        // Add real-time search with debouncing
        let searchTimeout;
        searchInput.addEventListener('input', function(e) {
          clearTimeout(searchTimeout);
          searchTimeout = setTimeout(() => {
            filterEquipmentTable();
          }, 300); // 300ms debounce
        });

        // Clear search on Escape key
        searchInput.addEventListener('keydown', function(e) {
          if (e.key === 'Escape') {
            searchInput.value = '';
            filterEquipmentTable();
          }
        });

        // Legacy Enter key functionality (keep for form submission if needed)
        searchInput.addEventListener('keyup', function (e) {
          if (e.key === 'Enter') {
            e.preventDefault(); // Prevent form submission since we have real-time search
          }
        });

        // Initial setup
        setTimeout(() => {
          filterEquipmentTable();
        }, 100);
      }

      // Function to view equipment details
      window.viewEquipmentDetails = function(id, name, type, category, quantity, image) {
        document.getElementById('view-equipment-id').textContent = id;
        document.getElementById('view-equipment-name').textContent = name;
        document.getElementById('view-equipment-type').textContent = type;
        document.getElementById('view-equipment-category').textContent = category;
        document.getElementById('view-equipment-quantity').textContent = quantity;
        document.getElementById('view-equipment-image').src = image;
        
        // Show the modal
        const modal = new bootstrap.Modal(document.getElementById('viewEquipmentModal'));
        modal.show();
      };

      // Refresh button functionality
      document.querySelector('.btn-refresh').addEventListener('click', function () {
        location.reload();
      });

      // Populate edit modal with data
      const editButtons = document.querySelectorAll('.btn-action.edit, .mobile-btn.edit');
      editButtons.forEach(button => {
        button.addEventListener('click', function () {
          const id = this.getAttribute('data-id');
          const type = this.getAttribute('data-type');
          const name = this.getAttribute('data-name');
          const quantity = this.getAttribute('data-quantity');
          const category = this.getAttribute('data-category');
          const image = this.getAttribute('data-image');

          document.getElementById('edit-id').value = id;
          document.getElementById('edit-type').value = type || '';
          document.getElementById('edit-name').value = name || '';
          document.getElementById('edit-quantity').value = quantity || '';
          document.getElementById('edit-category').value = category || '';

          // Handle image preview
          const currentImage = document.getElementById('current-image');
          const currentImageContainer = document.getElementById('current-image-container');
          if (image) {
            currentImage.src = '/' + image;
            currentImageContainer.style.display = 'block';
          } else {
            currentImageContainer.style.display = 'none';
          }

          // Update category based on type
          updateCategory();
        });
      });

      // Validate form on submit
      const editForm = document.querySelector('#editEquipmentModal form');
      editForm.addEventListener('submit', function (e) {
        const nameInput = document.getElementById('edit-name');
        const quantityInput = document.getElementById('edit-quantity');
        const imageInput = document.getElementById('edit-image');
        let errorMessage = '';

        if (!nameInput.value.trim()) {
          errorMessage = 'Equipment name is required';
          nameInput.focus();
          e.preventDefault();
        } else if (!quantityInput.value.match(/^\d+\s*(?:\w+)?$/)) {
          errorMessage = 'Quantity must be a number, optionally followed by a unit (e.g., "44" or "44 meter")';
          quantityInput.focus();
          e.preventDefault();
        } else if (imageInput.files.length > 0) {
          const file = imageInput.files[0];
          if (!file.type.startsWith('image/')) {
            errorMessage = 'Please upload a valid image';
            imageInput.focus();
            e.preventDefault();
          } else if (file.size > 5 * 1024 * 1024) {
            errorMessage = 'Image size must be less than 5MB';
            imageInput.focus();
            e.preventDefault();
          }
        }

        if (errorMessage) {
          alert(errorMessage);
        }
      });
    });

  </script>

