      /* From Uiverse.io by ilkhoeri */
      .card123 {
        --black: 0, 0%, 0%;
        --white-smoke: 0, 0%, 89%;
        --black-muted: 0, 0%, 13%;
        --green-light: 87, 73%, 63%;
        --gap: 12px;
        --h-card: 280px;
        --p-card: 1rem;
        --gap-action: 6px;
        --sz-action: 58px;
