                        <script>
                          const display = document.getElementById("numberDisplay");

                          const START_VALUE = 1500;
                          const ONE_DAY_MS = 24 * 60 * 60 * 1000;
                          const THIRTY_DAYS_MS = 30 * ONE_DAY_MS;

                          let data = JSON.parse(localStorage.getItem("counterData")) || {
                            number: START_VALUE,
                            startTime: Date.now(),
                            lastUpdate: Date.now()
                          };

                          const now = Date.now();

                          // Reset if 30 days passed since startTime
                          if (now - data.startTime >= THIRTY_DAYS_MS) {
                            data = {
                              number: 0,
                              startTime: now,
                              lastUpdate: now
                            };
                          }

                          // Check if a day passed since last update
                          if (now - data.lastUpdate >= ONE_DAY_MS) {
                            const daysPassed = Math.floor((now - data.lastUpdate) / ONE_DAY_MS);
                            for (let i = 0; i < daysPassed; i++) {
                              const increment = Math.random() < 0.5 ? 3 : 4;
                              data.number += increment;
                            }
                            data.lastUpdate += daysPassed * ONE_DAY_MS;
                          }

                          // Save updated data
                          localStorage.setItem("counterData", JSON.stringify(data));

                          // Display the number
                          display.innerText = data.number;
                        </script>
