      }

      /* Downward arrow */
      .tooltip-text::after {
        content: "";
        position: absolute;
        bottom: 100%;
        /* top of tooltip box */
        left: 50%;
        transform: translateX(-50%);
        border-width: 6px;
        border-style: solid;
