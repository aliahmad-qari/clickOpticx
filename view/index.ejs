    <script>
      // Icon Slider Script
      document.addEventListener('DOMContentLoaded', function () {
        const iconSlider = document.querySelector('#iconSlider');

        iconSlider.addEventListener('mouseenter', function () {
          $('#iconSlider').carousel('pause');
        });

        iconSlider.addEventListener('mouseleave', function () {
          $('#iconSlider').carousel('cycle');
        });
      });
      // Icon Slider Script

      // Payment Modal Script
      document.addEventListener("DOMContentLoaded", function () {
        const subscribeButtons = document.querySelectorAll(".pricing-card a");
        subscribeButtons.forEach(button => {
          button.addEventListener("click", function (event) {
            event.preventDefault();
            const card = this.closest(".pricing-card");
            const packageSpeed = card.querySelector(".pricing-price h4").textContent.replace("Mbps", "").trim();
            const packagePrice = card.querySelector(".pricing-option ul li:nth-child(2)").textContent.replace("RS.", "").trim();
            document.getElementById("selectedPackage").textContent = `Speed: ${packageSpeed} Mbps`;
            document.getElementById("package_name").value = packageSpeed;
            document.getElementById("amount").value = packagePrice;
            const paymentModal = new bootstrap.Modal(document.getElementById("paymentModal"));
            paymentModal.show();
          });
        });
      });

      // Card Container Swipe Script
      const cardContainer = document.getElementById("cardContainer");
      let cards = Array.from(document.querySelectorAll(".deal-card"));
      let startX, draggedCard;
      const initialCards = cards.map(card => card.outerHTML);

      updateCardPositions();

      cardContainer.addEventListener("mousedown", startDragging);
      cardContainer.addEventListener("touchstart", startDragging);

      function startDragging(e) {
        draggedCard = cards[0];
        startX = e.type === "touchstart" ? e.touches[0].clientX : e.clientX;
        document.addEventListener("mousemove", onDrag);
        document.addEventListener("touchmove", onDrag);
        document.addEventListener("mouseup", onRelease);
        document.addEventListener("touchend", onRelease);
      }

      function onDrag(e) {
        const currentX = e.type === "touchmove" ? e.touches[0].clientX : e.clientX;
        const moveX = currentX - startX;
        draggedCard.style.transform = `translateX(${moveX}px) rotate(${moveX / 50}deg) scale(1)`;
        draggedCard.style.zIndex = 10;
      }

      function onRelease(e) {
        const currentX = e.type === "touchend" ? e.changedTouches[0].clientX : e.clientX;
        const moveX = currentX - startX;
        if (Math.abs(moveX) > 150) {
          draggedCard.classList.add('swiped');
          draggedCard.style.transform = `translateX(${moveX > 0 ? '100vw' : '-100vw'}) rotate(${moveX / 50}deg)`;
          draggedCard.style.opacity = '0';
          setTimeout(() => {
            cardContainer.removeChild(draggedCard);
            cards.shift();
            if (cards.length === 0) {
              refillCards();
            }
            updateCardPositions();
          }, 500);
        } else {
          draggedCard.style.transform = `rotate(-5deg) scale(1)`;
        }
        cleanupEvents();
      }

      function refillCards() {
        initialCards.forEach(cardHTML => {
          const newCard = document.createElement('div');
          newCard.innerHTML = cardHTML;
          cardContainer.appendChild(newCard.firstChild);
        });
        cards = Array.from(document.querySelectorAll(".deal-card"));
        updateCardPositions();
      }

      function getRandomColor() {
        const letters = '0123456789ABCDEF';
        let color = '#';
        for (let i = 0; i < 6; i++) {
          color += letters[Math.floor(Math.random() * 16)];
        }
        return color;
      }

      function updateCardPositions() {
        cards.forEach((card, index) => {
          card.style.transition = 'all 0.5s ease';
          card.style.zIndex = cards.length - index;
          card.style.top = `${index * 10}px`;
          card.style.transform = `rotate(${-5 + (index * 2)}deg) scale(${1 - (index * 0.05)})`;
          if (index === 0) {
            card.style.background = `linear-gradient(135deg, ${getRandomColor()}, ${getRandomColor()})`;
          } else {
            card.style.background = `#${(111 + index * 111).toString(16).padStart(6, '0')}`;
          }
        });
      }

      function cleanupEvents() {
        document.removeEventListener("mousemove", onDrag);
        document.removeEventListener("touchmove", onDrag);
        document.removeEventListener("mouseup", onRelease);
        document.removeEventListener("touchend", onRelease);
      }
    </script>
