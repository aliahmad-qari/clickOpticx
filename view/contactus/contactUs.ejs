  <style>
    .contact-header {
      background-color: #494987;
      color: white;
      padding: 15px;
      text-align: center;
      font-size: 24px;
      font-weight: bold;
      border-radius: 5px 5px 0 0;
      position: relative;
      box-shadow: -4px 11px 6px rgba(0, 0, 0, 0.2);
    }

    .contact-container {
      color: white;
      padding: 20px;
      border-radius: 10px;
      box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.2);
      border-radius: 15px;
      padding: 20px;
      text-align: center;
    }

    .contact-btn {
      width: 100%;
      border: none;
      font-weight: bold;
      color: red;
      padding: 10px;
      border-radius: 5px;
      display: block;
      text-align: center;
      text-decoration: none;
      box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.2);
    }

    .social-icons {
      text-align: center;
    }

    .social-icons a {
      color: white;
      font-size: 24px;
      margin: 0 10px;
    }

    .shadow-box {
      box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.2);
      border-radius: 10px;
      padding: 20px;
      background: white;

    }

    .contact-icon {
      width: 70px;
      height: 70px;
      display: flex;
      justify-content: center;
      align-items: center;
      border-radius: 15px;
      background: white;
      box-shadow: 2px 2px 10px rgba(0, 0, 0, 0.1);
