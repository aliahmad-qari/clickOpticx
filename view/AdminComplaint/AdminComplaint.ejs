      }

      .stat-number {
        font-size: 1.3rem;
      }

      .stat-icon {
        font-size: 1.8rem;
      }

      .form-card {
        padding: 15px;
        margin: 5px;
      }

      .search-input {
        padding: 10px 10px 10px 35px;
        font-size: 0.9rem;
      }


    }

    /* Modal */
  </style>
